        <section class="section calender-section" id="calender-section">
            <div class="container h-100">
                <div class="calender-box">
                    <div class="user-calender-box">
                        <div class="row g-5">
                            <div class="col-xl-7 col-lg-6 col-md-6 col-sm-12 col-12">
                                <div class="calender-left-box">
                                    <div class="calender-user-img">
                                        <img src="~/assets/v2/img/user-1.png" alt="user-1" class="img-fluid">
                                    </div>
                                    <div class="calender-body">
                                        <h4>Alisa Thomas</h4>
                                        <p>Schedule an appointment with Alisa</p>
                                        <div class="calender-date-box">
                                            <div id="datepicker"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-5 col-lg-6 col-md-6 col-sm-12 col-12">
                                <div class="calender-right-box">
                                    <div class="right-tab-box">
                                        <h5>How long do you need?</h5>
                                        <div class="inner-tab-box mb-0">
                                            <ul class="nav nav-pills justify-content-start flex-nowrap" role="tablist">
                                                <li class="nav-item">
                                                    <a class="nav-link active" data-bs-toggle="tab" href="#tab1" role="tab">
                                                        15 Min
                                                    </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" data-bs-toggle="tab" href="#tab2" role="tab">
                                                        30 Min
                                                    </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" data-bs-toggle="tab" href="#tab3" role="tab">
                                                        01 Hr.
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="tab-content sedule-tab-content">
                                        <div class="form-group">
                                            <label class="form-label">What time works best?</label>
                                            <select class="form-select" aria-label="Default select example">
                                                <option selected>India Standard Time (6:32pm)</option>
                                                <option value="1">One</option>
                                                <option value="2">Two</option>
                                                <option value="3">Three</option>
                                            </select>
                                        </div>
                                        <div class="tab-pane active" id="tab1" role="tabpanel">
                                            <div class="inner-schdule-content">
                                                <div class="sedule-box">
                                                    <button type="button" class="btn sedule-btn">01:00 pm</button>
                                                    <button type="button" class="btn sedule-btn active">01:15 pm</button>
                                                    <button type="button" class="btn sedule-btn">01:30 pm</button>
                                                    <button type="button" class="btn sedule-btn">01:45 pm</button>
                                                    <button type="button" class="btn sedule-btn">02:00 pm</button>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- <div class="powerd-by-box">
                                            <img src="~/assets/v2/img/power-logo.png" alt="powerd-by-logo" class="img-fluid">
                                        </div> -->
                                        <div class="tab-pane fade" id="tab2" role="tabpanel">
                                            <div class="inner-schdule-content">
                                                <div class="sedule-box">
                                                    <button type="button" class="btn sedule-btn">01:00 pm</button>
                                                    <button type="button" class="btn sedule-btn active">01:30 pm</button>
                                                    <button type="button" class="btn sedule-btn">02:00 pm</button>
                                                    <button type="button" class="btn sedule-btn">02:30 pm</button>
                                                    <button type="button" class="btn sedule-btn">03:00 pm</button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="tab-pane fade" id="tab3" role="tabpanel">
                                            <div class="inner-schdule-content">
                                                <div class="sedule-box">
                                                    <button type="button" class="btn sedule-btn">01:00 pm</button>
                                                    <button type="button" class="btn sedule-btn active">02:00 pm</button>
                                                    <button type="button" class="btn sedule-btn">03:00 pm</button>
                                                    <button type="button" class="btn sedule-btn">04:00 pm</button>
                                                    <button type="button" class="btn sedule-btn">05:00 pm</button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="sedule-action-btn">
                                            <a href="#" class="btn cancle-btn">Cancel</a>
                                            <a href="#" class="btn add-btn" id="next-box">Next</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="powerd-by-box">
                            <img src="~/assets/v2/img/power-logo.png" alt="powerd-by-logo" class="img-fluid">
                        </div>
                    </div>
                    <div class="schedule-box">
                        <div class="section-title">
                            <h4>Schedule an appointment </h4>
                            <a href="#" class="back-btn" id="back_btn"><span><i class="bi bi-chevron-left"></i></span>Back</a>
                        </div>
                        <div class="row">
                            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 border-right">
                                <div class="schedule-card">
                                    <div class="schedule-card-img">
                                        <img src="~/assets/v2/img/user-img/sc-img.png" alt="sc-img" class="img-fluid">
                                    </div>
                                    <div class="schedule-card-body">
                                        <div class="schedule-card-title">
                                            <h4>Alisa Thomas</h4>
                                            <span>Schedule an appointment with Alisa </span>
                                        </div>
                                        <div class="schedule-card-content">
                                            <h5>appointment Date and time</h5>
                                            <ul class="schedule-list">
                                                <li>
                                                    <span class="icon-img">
                                                        <img src="~/assets/v2/img/icon/clock-img.png" alt="clock-img" class="img-fluid light-icon">
                                                        <img src="~/assets/v2/img/icon/clock-img_dark.png" alt="clock-img" class="img-fluid dark-icon">
                                                    </span>
                                                    <p>15 Mins</p>
                                                </li>
                                                <li>
                                                    <span class="icon-img">
                                                        <img src="~/assets/v2/img/icon/calendar_month.png" alt="calendar_month" class="img-fluid light-icon">
                                                        <img src="~/assets/v2/img/icon/calendar_month1.png" alt="calendar_month" class="img-fluid dark-icon">
                                                    </span>
                                                    <p>01:45 pm, Thu, Sept 20, 2024</p>
                                                </li>
                                                <li>
                                                    <span class="icon-img">
                                                        <img src="~/assets/v2/img/icon/world-img.png" alt="world-img" class="img-fluid light-icon">
                                                        <img src="~/assets/v2/img/icon/world-img1.png" alt="world-img" class="img-fluid dark-icon">
                                                    </span>
                                                    <p>India Standard Time <span>(UTC+05:30)</span></p>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                                <div class="schedule-right-box">
                                    <div class="schedule-box-title">
                                        <h4>Enter Details</h4>
                                    </div>
                                    <div class="schedule-form-box">
                                        <form class="qus-form schedule-form" action="" method="get">
                                            <div class="form-group">
                                                <label class="form-label">Name</label>
                                                <input type="text" class="form-control" placeholder="Enter Name">
                                            </div>
                                            <div class="form-group">
                                                <label class="form-label">Email Address </label>
                                                <input type="email" class="form-control" placeholder="youremail@gmail.com">
                                            </div>
                                            <div class="form-group">
                                                <p>By proceeding, you confirm that you have read and agree to Eyaana's <a href="#">Terms of Use</a> and <a href="#">Privacy Notice</a>.</p>
                                            </div>
                                            <div class="form-group sedule-action-btn">
                                                <a href="#" class="btn cancle-btn">Cancel</a>
                                                <a href="#" class="btn add-btn" id="schedule_btn">Schedule</a>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="powerd-by-box">
                            <img src="~/assets/v2/img/power-logo.png" alt="powerd-by-logo" class="img-fluid">
                        </div>
                    </div>
                    <div class="thank-you-box" style="background-image: url(~/assets/v2/img/thankyou-bg.png);">
                        <div class="thank-you-content">
                            <div class="thank-you-card">
                                <img src="~/assets/v2/img/icon/check-mark.png" alt="check-mark" class="img-fluid thank-you-img">
                                <div class="thank-you-card-body">
                                    <h2>Thank You</h2>
                                    <p>for booking an appointment with us We'll be in touch to confirm closer to the date</p>
                                </div>
                            </div>
                            <div class="thank-you-schdule-box">
                                <h5>appointment Details</h5>
                                <div class="row align-items-center">
                                    <div class="col-lg-4 col-md-4 col-sm-6 col-12 col">
                                        <div class="appointment-list">
                                            <div class="appointment-box-content">
                                                <span class="icon-img">
                                                    <img src="~/assets/v2/img/icon/clock-img.png" alt="clock-img" class="img-fluid light-icon">
                                                    <img src="~/assets/v2/img/icon/clock-img_dark.png" alt="clock-img" class="img-fluid dark-icon">
                                                </span>
                                                <p>15 Mins</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-6 col-12 col">
                                        <div class="appointment-list">
                                            <div class="appointment-box-content">
                                                <span class="icon-img">
                                                    <img src="~/assets/v2/img/icon/calendar_month.png" alt="calendar_month" class="img-fluid light-icon">
                                                    <img src="~/assets/v2/img/icon/calendar_month1.png" alt="calendar_month" class="img-fluid dark-icon">
                                                </span>
                                                <p>01:45 pm, Thu, Sept 20, 2024</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-sm-6 col-12 col p-0">
                                        <div class="appointment-list">
                                            <div class="appointment-box-content">
                                                <span class="icon-img">
                                                    <img src="~/assets/v2/img/icon/world-img.png" alt="world-img" class="img-fluid light-icon">
                                                    <img src="~/assets/v2/img/icon/world-img1.png" alt="world-img" class="img-fluid dark-icon">
                                                </span>
                                                <p>India Standard Time <span>(UTC+05:30)</span></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- <ul class="schedule-list">
                                    <li>

                                    </li>
                                    <li>

                                    </li>
                                    <li>

                                    </li>
                                </ul> -->
                            </div>
                        </div>
                        <div class="powerd-by-box">
                            <img src="~/assets/v2/img/power-logo.png" alt="powerd-by-logo" class="img-fluid">
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- -------------------------------------------------------------- -->
        <!-- Login box.scss -->
        <!-- -------------------------------------------------------------- -->
    </div>
