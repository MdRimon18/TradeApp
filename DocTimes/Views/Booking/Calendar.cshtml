    <script>
        $(function () {
            $("#datepicker").datepicker();
        });
    </script>

    <script>
        $(document).ready(function () {

            $('#back_btn').click(function () {
                // $('.notification-box').toggle('slideRight');
                $('.schedule-box').removeClass("show");
                $('.user-calender-box').removeClass("hide");
            });

            $('#next-box').click(function () {
                // $('.notification-box').toggle('slideLeft');
                $(".schedule-box").addClass("show");
                $(".user-calender-box").addClass("hide");
            });
            $('#schedule_btn').click(function () {
                // $('.notification-box').toggle('slideLeft');
                $(".thank-you-box").addClass("show");
                $('.schedule-box').removeClass("show");
                $(".user-calender-box").addClass("hide");
            });
        });
    </script>
</body>

</html>